{"code":"import teams from '@nhl-api/teams';\r\nimport { throwError, handleUrl, get } from '../util';\r\nexport default async function getTeams(options) {\r\n    const baseUrl = handleUrl('teams', options);\r\n    const url = options && options.expand && options.expand.includes('roster')\r\n        ? `${baseUrl}/roster`\r\n        : options && options.expand && options.expand.includes('stats')\r\n            ? `${baseUrl}/stats`\r\n            : baseUrl;\r\n    if (options && options.expand) {\r\n        options.expand = `team.${options.expand}`;\r\n    }\r\n    try {\r\n        const response = await get(url, options).then((data) => {\r\n            if (Array.isArray(data) && data.some(d => d.abbreviation)) {\r\n                return data.map((d, i) => Object.assign({}, d, teams[i]));\r\n            }\r\n            else if (data.hasOwnProperty('abbreviation')) {\r\n                return Object.assign({}, data, teams.find(team => team.id === data.id));\r\n            }\r\n            if (data.roster) {\r\n                return data.roster;\r\n            }\r\n            if (data.stats) {\r\n                return data.stats;\r\n            }\r\n            return data;\r\n        });\r\n        return Promise.resolve(response);\r\n    }\r\n    catch (err) {\r\n        return throwError('getTeams', err);\r\n    }\r\n}\r\n//# sourceMappingURL=index.js.map","references":["/Users/sarapope/dev/nhl-api2/packages/teams/dist/index.d.ts","/Users/sarapope/dev/nhl-api2/packages/client/src/util/index.ts"],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/teams/index.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,MAAM,gBAAgB,CAAC;AACnC,OAAO,EAAE,UAAU,EAAE,SAAS,EAAe,GAAG,EAAE,MAAM,SAAS,CAAC;AAOlE,MAAM,CAAC,OAAO,CAAC,KAAK,UAAU,QAAQ,CAAC,OAAqB;IAC1D,MAAM,OAAO,GAAG,SAAS,CAAC,OAAO,EAAE,OAAO,CAAC,CAAC;IAE5C,MAAM,GAAG,GACP,OAAO,IAAI,OAAO,CAAC,MAAM,IAAI,OAAO,CAAC,MAAM,CAAC,QAAQ,CAAC,QAAQ,CAAC;QAC5D,CAAC,CAAC,GAAG,OAAO,SAAS;QACrB,CAAC,C