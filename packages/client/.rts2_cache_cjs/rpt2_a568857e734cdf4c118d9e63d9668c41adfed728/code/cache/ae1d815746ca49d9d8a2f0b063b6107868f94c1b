{"code":"import teams from '@nhl-api/teams';\r\nimport { api, throwError } from '../util';\r\nexport default async function getTeams(options) {\r\n    if (options && options.season && options.season.toString().length !== 8) {\r\n        throwError('getTeams', `Season must be formatted as both full years, i.e. '20192020'.`);\r\n    }\r\n    const id = options && options.name\r\n        ? teams.filter((team) => team.name === options.name)[0].id\r\n        : options && options.id;\r\n    const baseUrl = (options && options.id) || (options && options.name)\r\n        ? `/teams/${id}`\r\n        : `/teams`;\r\n    const url = options && options.expand && options.expand.includes('roster')\r\n        ? `${baseUrl}/roster`\r\n        : options && options.expand && options.expand.includes('stats')\r\n            ? `${baseUrl}/stats`\r\n            : baseUrl;\r\n    if (options && options.expand) {\r\n        options.expand = `team.${options.expand}`;\r\n    }\r\n    const activeTeams = teams.filter((team) => team.isActive);\r\n    try {\r\n        const response = await api\r\n            /// @ts-ignore\r\n            .get(url, options ? { params: { ...options } } : null)\r\n            .then((response) => response.data)\r\n            .then((data) => {\r\n            if (data.roster) {\r\n                return data.roster;\r\n            }\r\n            if (data.stats) {\r\n                return data.stats.length > 1 ? data.stats : data.stats[0];\r\n            }\r\n            if (data.teams.length > 1 &&\r\n                data.teams.some((d) => d.abbreviation)) {\r\n                return data.teams.map((d, i) => Object.assign({}, d, activeTeams[i]));\r\n            }\r\n            if (data.teams[0].hasOwnProperty('abbreviation')) {\r\n                return Object.assign({}, data.teams[0], activeTeams.find((team) => team.id === data.teams[0].id));\r\n            }\r\n            return data;\r\n        });\r\n        return Promise.resolve(response);\r\n    }\r\n    catch (err) {\r\n        return throwError('getTeams', err);\r\n    }\r\n}\r\n//# sourceMappingURL=index.js.map","references":["/Users/sara/dev/nhlapi2/packages/teams/dist/index.d.ts","/Users/sara/dev/nhlapi2/packages/client/src/util/index.ts","/Users/sara/dev/nhlapi2/node_modules/axios/index.d.ts"],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/teams/index.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,MAAM,gBAAgB,CAAC;AACnC,OAAO,EAAE,GAAG,EAAE,UAAU,EAAE,MAAM,SAAS,CAAC;AAS1C,MAAM,CAAC,OAAO,CAAC,KAAK,UAAU,QAAQ,CAAC,OAAqB;IAC1D,IAAI,OAAO,IAAI,OAAO,CAAC,MAAM,IAAI,OAAO,CAAC,MAAM,CAAC,QAAQ,EAAE,CAAC,MAAM,KAAK,CAAC,EAAE;QACvE,UAAU,CACR,UAAU,EACV,+DAA+D,CAChE,CAAC;KACH;IAED,MAAM,EAAE,GACN,OAAO,IAAI,OAAO,CAAC,IAAI;QACrB,CAAC,CAAC,KAAK,CAAC,MAAM,CAAC,CAAC,IAAS,EAAE,EAAE,CAAC,IAAI,CAAC,IAAI,KAAK,OAAO,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;QAC/D,CAAC,CAAC,OAAO,IAAI,OAAO,CAAC,EAAE,CAAC;IAC5B,MAAM,OAAO,GACX,CAAC,OAAO,IAAI,OAAO,CAAC,EAAE,CAAC,IAAI,CAAC,OAAO,IAAI,OAAO,CAAC,IAAI,CAAC;QAClD,CAAC,CAAC,UAAU,EAAE,EAAE;QAChB,CAAC,CAAC,QAAQ,CAAC;IAEf,MAAM,GAAG,GACP,OAAO,IAAI,OAAO,CAAC,MAAM,IAAI,OAAO,CAAC,MAAM,CAAC,QAAQ,CAAC,QAAQ,CAAC;QAC5D,CAAC,CAAC,GAAG,OAAO,SAAS;QACrB,CAAC,CAAC,OAAO,IAAI,OAAO,CAAC,MAAM,IAAI,OAAO,CAAC,MAAM,CAAC,QAAQ,CAAC,OAAO,CAAC;YAC/D,CAAC,CAAC,GAAG,OAAO,QAAQ;YACpB,CAAC,CAAC,OAAO,CAAC;IAEd,IAAI,OAAO,IAAI,OAAO,CAAC,MAAM,EAAE;QAC7B,OAAO,CAAC,MAAM,GAAG,QAAQ,OAAO,CAAC,MAAM,EAAE,CAAC;KAC3C;IAED,MAAM,WAAW,GAAG,KAAK,CAAC,MAAM,CAAC,CAAC,IAAS,EAAE,EAAE,CAAC,IAAI,CAAC,QAAQ,CAAC,CAAC;IAE/D,IAAI;QACF,MAAM,QAAQ,GAAG,MAAM,GAAG;YACxB,cAAc;aACb,GAAG,CAAC,GAAG,EAAE,OAAO,CAAC,CAAC,CAAC,EAAE,MAAM,EAAE,EAAE,GAAG,OAAO,EAAE,EAAE,CAAC,CAAC,CAAC,IAAI,CAAC;aACrD,IAAI,CAAC,CAAC,QAAuB,EAAE,EAAE,CAAC,QAAQ,CAAC,IAAI,CAAC;aAChD,IAAI,CAAC,CAAC,IAAS,EAAE,EAAE;YAClB,IAAI,IAAI,CAAC,MAAM,EAAE;gBACf,OAAO,IAAI,CAAC,MAAM,CAAC;aACpB;YACD,IAAI,IAAI,CAAC,KAAK,EAAE;gBACd,OAAO,IAAI,CAAC,KAAK,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC,CAAC,IAAI,CAAC,KAAK,CAAC,CAAC,CAAC,IA